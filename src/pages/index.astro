---
import Layout from '../layouts/Layout.astro';

const title = "QR Code Generator"
---

<Layout title={title}>
	<main class="min-h-screen grid place-content-center">
		<div class="bg-white rounded-2xl w-80 p-4 flex flex-col gap-4 text-center">
			<picture class="self-center">
				<img class="overflow-hidden" src="data:image/png;base64," />
			</picture>
			<h1 class="text-2xl font-bold">{title}</h1>
		</div>
	</main>
</Layout>
